{
	"name": "@cosmixclub/parsero",
	"version": "1.0.0",
	"description": "Construtor de agentes de IA de maneira simplificada.",
	"author": "Gepetojj <contato.gepetojj@gmail.com>",
	"license": "MIT",
	"main": "dist/index.js",
	"packageManager": "pnpm@10.8.0",
	"keywords": [
		"crewai",
		"langchain",
		"langgraph"
	],
	"homepage": "https://github.com/cosmixclub/parsero",
	"bugs": {
		"url": "https://github.com/cosmixclub/parsero/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cosmixclub/parsero.git"
	},
	"files": [
		"dist"
	],
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./dist/index.js",
			"default": "./dist/index.cjs"
		}
	},
	"scripts": {
		"ci": "pnpm build && pnpm check-format && pnpm check-exports",
		"build": "tsup",
		"lint": "eslint --fix .",
		"format": "prettier --write .",
		"check-format": "prettier --check .",
		"commit": "git cz",
		"check-exports": "attw --pack .",
		"local-release": "changeset version && changeset publish",
		"prepublishOnly": "pnpm run ci"
	},
	"pnpm": {
		"overrides": {
			"@langchain/core": "0.3.44"
		},
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"@langchain/core": "0.3.44",
		"@langchain/langgraph": "^0.2.63",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.17.4",
		"@changesets/cli": "^2.28.1",
		"@langchain/community": "^0.3.40",
		"@langchain/openai": "^0.5.5",
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/node": "^22.14.1",
		"@typescript-eslint/eslint-plugin": "^8.29.1",
		"@typescript-eslint/parser": "^8.29.1",
		"@vercel/style-guide": "^6.0.0",
		"commitizen": "^4.3.1",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv": "^16.5.0",
		"eslint": "8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-perfectionist": "^4.11.0",
		"eslint-plugin-prettier": "^5.2.6",
		"prettier": "^3.5.3",
		"tsup": "^8.4.0",
		"tsx": "^4.19.3",
		"typescript": "^5.8.3"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
